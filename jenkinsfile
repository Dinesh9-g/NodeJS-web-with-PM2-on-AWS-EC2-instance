pipeline {

  agent any 

  stages {
    stages('Install Dependencies')
       steps {
          sh 'npm install'
    }
}

stage('Test') {
  steps {
    sh 'echo "testing application...."'
}  
  }


  stage("Display nodejs application")
   steps {

     sh 'echo "deploying application ...."'
 }

}
   }

}






