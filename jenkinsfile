pipeline {
    agent any

    stages {
        stage('Install Dependencies') {
            steps {
                bat 'npm install'
            }
        }

        stage('Test') {
            steps {
                bat 'echo "testing application."'
            }
        }

        stage('Deploy Node.js Application') {
            steps {
                bat 'echo "deploying application."'
            }
        }
    }
}


